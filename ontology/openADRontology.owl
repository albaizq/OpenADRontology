@prefix : <https://w3id.org/def/openadr#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix oadr: <https://w3id.org/def/openadr> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/def/openadr> .

<https://w3id.org/def/openadr> rdf:type owl:Ontology ;
                                <http://purl.org/dc/elements/1.1/creator> "Alba Fern√°ndez Izquierdo" ;
                                owl:versionInfo "0.0.3" ;
                                <http://purl.org/dc/elements/1.1/creator> "Andrea Cimmino" ;
                                rdfs:comment "http://purl.org/NET/rdflicense/cc-by4.0" ;
                                <http://purl.org/dc/terms/license> <http://purl.org/NET/rdflicense/cc-by4.0> ;
                                rdfs:comment "This ontology describes the openADR protocol based on the documentation provided by the openADR Profile B" ;
                                <http://purl.org/dc/elements/1.1/creator> <http://www.garcia-castro.com/foaf.rdf#me> ;
                                <http://purl.org/dc/terms/title> "OpenADR ontology" .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/elements/1.1/creator
<http://purl.org/dc/elements/1.1/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/title
<http://purl.org/dc/terms/title> rdf:type owl:AnnotationProperty .


###  http://purl.org/vocab/vann/preferredNamespaceUri
<http://purl.org/vocab/vann/preferredNamespaceUri> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/def/openadr#SignalIntervalDuration
:SignalIntervalDuration rdf:type owl:ObjectProperty ;
                        rdfs:range <http://www.w3.org/2006/time#Duration> ;
                        rdfs:comment "Duration of a signal interval" ;
                        rdfs:label "interval duration" .


###  https://w3id.org/def/openadr#eventDescriptor
:eventDescriptor rdf:type owl:ObjectProperty ;
                 rdfs:range :EventDescriptor ;
                 rdfs:comment "Relation between an event and its metadata" ;
                 rdfs:label "event descriptor" .


###  https://w3id.org/def/openadr#expectedResponse
:expectedResponse rdf:type owl:ObjectProperty ;
                  rdfs:range :Opt ;
                  rdfs:label "expected response" .


###  https://w3id.org/def/openadr#hasActivePeriod
:hasActivePeriod rdf:type owl:ObjectProperty ;
                 rdfs:range <http://www.w3.org/2006/time#Interval> ;
                 rdfs:comment "Relation between an entity an its available period" ;
                 rdfs:label "has active period" .


###  https://w3id.org/def/openadr#hasEventStatus
:hasEventStatus rdf:type owl:ObjectProperty ;
                rdfs:range :Event ;
                rdfs:label "has event" .


###  https://w3id.org/def/openadr#hasPayloadReourceStatus
:hasPayloadReourceStatus rdf:type owl:ObjectProperty ;
                         rdfs:range :PayloadResourceStatus ;
                         rdfs:comment "Relation between an entity and the payload resource status" ;
                         rdfs:label "has payload resource status" .


###  https://w3id.org/def/openadr#hasReportPayload
:hasReportPayload rdf:type owl:ObjectProperty ;
                  rdfs:domain :Report ;
                  rdfs:range :ReportPayload ;
                  rdfs:comment "Relation between a report and its payload" ;
                  rdfs:label "has report payload" .


###  https://w3id.org/def/openadr#hasSamplingRate
:hasSamplingRate rdf:type owl:ObjectProperty ;
                 rdfs:domain :Report ;
                 rdfs:range :SamplingRate ;
                 rdfs:comment "Indicates the sampling rate of a report" ;
                 rdfs:label "has sampling rate" .


###  https://w3id.org/def/openadr#hasSignal
:hasSignal rdf:type owl:ObjectProperty ;
           rdfs:range :Signal ;
           rdfs:comment "Relation between an entity and its signal" ;
           rdfs:label "has signal" .


###  https://w3id.org/def/openadr#hasSignalInterval
:hasSignalInterval rdf:type owl:ObjectProperty ;
                   rdfs:range :SignalInterval ;
                   rdfs:label "has interval" .


###  https://w3id.org/def/openadr#hasTarget
:hasTarget rdf:type owl:ObjectProperty ;
           rdfs:range :Target ;
           rdfs:comment "Relation between an entity and its target" ;
           rdfs:label "has target" .


###  https://w3id.org/def/openadr#hasTypicalValue
:hasTypicalValue rdf:type owl:ObjectProperty ;
                 rdfs:domain :SignalInterval ;
                 rdfs:range :Value ;
                 rdfs:comment "Typical value of an interval" ;
                 rdfs:label "has typical value" .


###  https://w3id.org/def/openadr#isMeasuredIn
:isMeasuredIn rdf:type owl:ObjectProperty ;
              rdfs:comment "Relation to indicate the units of a measurement" ;
              rdfs:label "is measured in" .


###  https://w3id.org/def/openadr#manages
:manages rdf:type owl:ObjectProperty ;
         rdfs:comment "Relation between a DVN and its FEID" ;
         rdfs:label "manages" .


###  https://w3id.org/def/openadr#oadrMaxPeriod
:oadrMaxPeriod rdf:type owl:ObjectProperty ;
               rdfs:range <http://www.w3.org/2006/time#Interval> ;
               rdfs:label "max period" .


###  https://w3id.org/def/openadr#oadrMinPeriod
:oadrMinPeriod rdf:type owl:ObjectProperty ;
               rdfs:range <http://www.w3.org/2006/time#Interval> ;
               rdfs:label "min period" .


###  https://w3id.org/def/openadr#ramp_up
:ramp_up rdf:type owl:ObjectProperty ;
         rdfs:label "ramp up" .


###  https://w3id.org/def/openadr#recovery
:recovery rdf:type owl:ObjectProperty ;
          rdfs:label "recovery" .


###  https://w3id.org/def/openadr#reportDataSource
:reportDataSource rdf:type owl:ObjectProperty ;
                  rdfs:label "report data source" .


###  https://w3id.org/def/openadr#reportDescriptor
:reportDescriptor rdf:type owl:ObjectProperty ;
                  rdfs:range :ReportDescriptor ;
                  rdfs:label "report descriptor" .


###  https://w3id.org/def/openadr#reportSubject
:reportSubject rdf:type owl:ObjectProperty ;
               rdfs:label "report subject" .


###  https://w3id.org/def/openadr#sendEvent
:sendEvent rdf:type owl:ObjectProperty ;
           rdfs:label "send" .


#################################################################
#    Data properties
#################################################################

###  https://w3id.org/def/openadr#accuracy
:accuracy rdf:type owl:DatatypeProperty ;
          rdfs:range xsd:string ;
          rdfs:label "accuracy" .


###  https://w3id.org/def/openadr#confidence
:confidence rdf:type owl:DatatypeProperty ;
            rdfs:range xsd:string ;
            rdfs:label "confidence" .


###  https://w3id.org/def/openadr#createdDateTime
:createdDateTime rdf:type owl:DatatypeProperty ;
                 rdfs:range xsd:dateTimeStamp ;
                 rdfs:label "createdDateTime" .


###  https://w3id.org/def/openadr#dataquality
:dataquality rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string ;
             rdfs:label "data quality" .


###  https://w3id.org/def/openadr#dstart
:dstart rdf:type owl:DatatypeProperty ;
        rdfs:range xsd:dateTimeStamp ;
        rdfs:label "dstart" .


###  https://w3id.org/def/openadr#eventName
:eventName rdf:type owl:DatatypeProperty ;
           rdfs:domain :Event ;
           rdfs:range owl:rational ;
           rdfs:comment "Name fo an event" ;
           rdfs:label "event name" .


###  https://w3id.org/def/openadr#groupName
:groupName rdf:type owl:DatatypeProperty ;
           rdfs:subPropertyOf owl:topDataProperty ;
           rdfs:range xsd:string ;
           rdfs:label "group name" .


###  https://w3id.org/def/openadr#isOnChange
:isOnChange rdf:type owl:DatatypeProperty ;
            rdfs:domain :SamplingRate ;
            rdfs:range xsd:boolean ;
            rdfs:label "is on change" .


###  https://w3id.org/def/openadr#marketContext
:marketContext rdf:type owl:DatatypeProperty ;
               rdfs:range xsd:string ;
               rdfs:label "market context" .


###  https://w3id.org/def/openadr#payloadFloat
:payloadFloat rdf:type owl:DatatypeProperty ;
              rdfs:range xsd:float ;
              rdfs:label "paylaod float" .


###  https://w3id.org/def/openadr#readingType
:readingType rdf:type owl:DatatypeProperty ;
             rdfs:range xsd:string ;
             rdfs:label "reading type" .


###  https://w3id.org/def/openadr#reportName
:reportName rdf:type owl:DatatypeProperty ;
            rdfs:domain :Report ;
            rdfs:range xsd:string ;
            rdfs:label "report name" .


###  https://w3id.org/def/openadr#signalName
:signalName rdf:type owl:DatatypeProperty ;
            rdfs:domain :Signal ;
            rdfs:range xsd:string ;
            rdfs:label "signal name" .


###  https://w3id.org/def/openadr#venName
:venName rdf:type owl:DatatypeProperty ;
         rdfs:domain :VEN ;
         rdfs:range owl:rational ;
         rdfs:label "VEN name" .


###  https://w3id.org/def/openadr#version
:version rdf:type owl:DatatypeProperty ;
         rdfs:range xsd:string ;
         rdfs:label "version" .


###  https://w3id.org/def/openadr#vtnName
:vtnName rdf:type owl:DatatypeProperty ;
         rdfs:domain :VTN ;
         rdfs:range owl:rational ;
         rdfs:label "VTN name" .


#################################################################
#    Classes
#################################################################

###  http://www.w3.org/2006/time#Duration
<http://www.w3.org/2006/time#Duration> rdf:type owl:Class ;
                                       rdfs:label "Duration" .


###  http://www.w3.org/2006/time#Instant
<http://www.w3.org/2006/time#Instant> rdf:type owl:Class ;
                                      rdfs:subClassOf <http://www.w3.org/2006/time#TemporalEntity> .


###  http://www.w3.org/2006/time#Interval
<http://www.w3.org/2006/time#Interval> rdf:type owl:Class ;
                                       rdfs:subClassOf <http://www.w3.org/2006/time#TemporalEntity> .


###  http://www.w3.org/2006/time#TemporalEntity
<http://www.w3.org/2006/time#TemporalEntity> rdf:type owl:Class .


###  http://www.wurvoc.org/vocabularies/om-1.8/Unit_of_measure
<http://www.wurvoc.org/vocabularies/om-1.8/Unit_of_measure> rdf:type owl:Class .


###  https://w3id.org/def/openadr#Asset
:Asset rdf:type owl:Class ;
       rdfs:comment """A type of Resource that represents a specific collection of physical loads.
Resources can be composed of Assets, and an Asset may be Resource, but Assets cannot be further decomposed into multiple Assets or Resources.""" ;
       rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
       rdfs:label "Asset" .


###  https://w3id.org/def/openadr#BidEnergySignal
:BidEnergySignal rdf:type owl:Class ;
                 rdfs:subClassOf :Signal ;
                 rdfs:label "Bid energy signal" .


###  https://w3id.org/def/openadr#BidLoadSignal
:BidLoadSignal rdf:type owl:Class ;
               rdfs:subClassOf :Signal ;
               rdfs:label "Bid load signal" .


###  https://w3id.org/def/openadr#BidPriceSignal
:BidPriceSignal rdf:type owl:Class ;
                rdfs:subClassOf :Signal ;
                rdfs:label "Bid price signal" .


###  https://w3id.org/def/openadr#ChargeStateSignal
:ChargeStateSignal rdf:type owl:Class ;
                   rdfs:subClassOf :Signal ;
                   rdfs:label "Charge state signal" .


###  https://w3id.org/def/openadr#DataReport
:DataReport rdf:type owl:Class ;
            rdfs:subClassOf :Report ;
            rdfs:label "Data report" .


###  https://w3id.org/def/openadr#DemandChargeSignal
:DemandChargeSignal rdf:type owl:Class ;
                    rdfs:subClassOf :Signal ;
                    rdfs:label "Demand charge signal" .


###  https://w3id.org/def/openadr#ElectricityPriceSignal
:ElectricityPriceSignal rdf:type owl:Class ;
                        rdfs:subClassOf :Signal ;
                        rdfs:label "Electricity price signal" .


###  https://w3id.org/def/openadr#EnergyPriceSignal
:EnergyPriceSignal rdf:type owl:Class ;
                   rdfs:subClassOf :Signal ;
                   rdfs:label "Energy price signal" .


###  https://w3id.org/def/openadr#Event
:Event rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :eventDescriptor ;
                         owl:someValuesFrom :EventDescriptor
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :expectedResponse ;
                         owl:someValuesFrom :Opt
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasSignal ;
                         owl:someValuesFrom :Signal
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :ramp_up ;
                         owl:someValuesFrom <http://www.w3.org/2006/time#Duration>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :recovery ;
                         owl:someValuesFrom <http://www.w3.org/2006/time#Duration>
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :hasEventStatus ;
                         owl:allValuesFrom :EventStatus
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :createdDateTime ;
                         owl:someValuesFrom owl:rational
                       ] ,
                       [ rdf:type owl:Restriction ;
                         owl:onProperty :version ;
                         owl:allValuesFrom owl:rational
                       ] ;
       rdfs:comment "An event is a notification from the utility to demand side resources requesting load shed starting at a specific time, over a specified duration, and may include targeting information designating specific resources that should participate in the event" ;
       rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
       rdfs:label "Event" .


###  https://w3id.org/def/openadr#EventDescriptor
:EventDescriptor rdf:type owl:Class ;
                 rdfs:comment "Part of the OpenADR event object that describes metadata about the event, such as program name and event priority" ;
                 rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
                 rdfs:label "Event descriptor" .


###  https://w3id.org/def/openadr#EventStatus
:EventStatus rdf:type owl:Class ;
             rdfs:comment "Status of an OpenADR event" ;
             rdfs:label "Event status" .


###  https://w3id.org/def/openadr#GreenButtonReport
:GreenButtonReport rdf:type owl:Class ;
                   rdfs:subClassOf :HistoryReport ;
                   rdfs:label "Green button report" .


###  https://w3id.org/def/openadr#HistoryReport
:HistoryReport rdf:type owl:Class ;
               rdfs:subClassOf :DataReport ;
               rdfs:label "History report" .


###  https://w3id.org/def/openadr#HistoryUsageReport
:HistoryUsageReport rdf:type owl:Class ;
                    rdfs:subClassOf :HistoryReport ;
                    rdfs:label "History usage report" .


###  https://w3id.org/def/openadr#ItemBase
:ItemBase rdf:type owl:Class ;
          rdfs:comment "Units for the data in the report" ;
          rdfs:label "Item base" .


###  https://w3id.org/def/openadr#LoadControlSignal
:LoadControlSignal rdf:type owl:Class ;
                   rdfs:subClassOf :Signal ;
                   rdfs:label "Load control signal" .


###  https://w3id.org/def/openadr#LoadDispatchSignal
:LoadDispatchSignal rdf:type owl:Class ;
                    rdfs:subClassOf :Signal ;
                    rdfs:label "Load dispatch signal" .


###  https://w3id.org/def/openadr#MetadataReport
:MetadataReport rdf:type owl:Class ;
                rdfs:subClassOf :Report ;
                rdfs:label "Metadata report" .


###  https://w3id.org/def/openadr#Opt
:Opt rdf:type owl:Class ;
     rdfs:comment "The expected response from the demand side resource owner upon receipt of an event." ;
     rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
     rdfs:label "Opt" .


###  https://w3id.org/def/openadr#OptIn
:OptIn rdf:type owl:Class ;
       rdfs:subClassOf :Opt ;
       rdfs:label "Opt In" .


###  https://w3id.org/def/openadr#OptOut
:OptOut rdf:type owl:Class ;
        rdfs:subClassOf :Opt ;
        rdfs:label "Opt Out" .


###  https://w3id.org/def/openadr#PayloadResourceStatus
:PayloadResourceStatus rdf:type owl:Class ;
                       rdfs:comment "Status of the payload resource" ;
                       rdfs:label "Payload resource status" .


###  https://w3id.org/def/openadr#Report
:Report rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasActivePeriod ;
                          owl:someValuesFrom <http://www.w3.org/2006/time#Interval>
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasReportPayload ;
                          owl:someValuesFrom :ReportPayload
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :reportDescriptor ;
                          owl:someValuesFrom :ReportDescriptor
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasSamplingRate ;
                          owl:allValuesFrom :SamplingRate
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :createdDateTime ;
                          owl:someValuesFrom owl:rational
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :dstart ;
                          owl:someValuesFrom owl:rational
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :reportName ;
                          owl:someValuesFrom owl:rational
                        ] ;
        rdfs:comment "Report sent from a VEN to a VTN to report the energy consumption or the status of Resources connected to the VEN" ;
        rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
        rdfs:label "Report" .


###  https://w3id.org/def/openadr#ReportDescriptor
:ReportDescriptor rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Restriction ;
                                    owl:onProperty :reportDataSource ;
                                    owl:someValuesFrom :Target
                                  ] ,
                                  [ rdf:type owl:Restriction ;
                                    owl:onProperty :reportDescriptor ;
                                    owl:someValuesFrom :Target
                                  ] ;
                  rdfs:comment "Part of the OpenADR report object that describes metadata about the report" ;
                  rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
                  rdfs:label "Report descriptor" .


###  https://w3id.org/def/openadr#ReportPayload
:ReportPayload rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Restriction ;
                                 owl:onProperty :hasPayloadReourceStatus ;
                                 owl:allValuesFrom :PayloadResourceStatus
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :accuracy ;
                                 owl:someValuesFrom owl:rational
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :confidence ;
                                 owl:someValuesFrom owl:rational
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :dataquality ;
                                 owl:someValuesFrom owl:rational
                               ] ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty :payloadFloat ;
                                 owl:someValuesFrom owl:rational
                               ] ;
               rdfs:label "Report payload" .


###  https://w3id.org/def/openadr#Resource
:Resource rdf:type owl:Class ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty :manages ;
                            owl:someValuesFrom :Asset
                          ] ;
          rdfs:comment "Entity that is enrolled in the DR Programs and is capable of delivering some sort of change to their load profile in response to receiving a DR signal from a VTN." ;
          rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
          rdfs:label "Resource" .


###  https://w3id.org/def/openadr#SamplingRate
:SamplingRate rdf:type owl:Class ;
              rdfs:subClassOf [ rdf:type owl:Restriction ;
                                owl:onProperty :oadrMaxPeriod ;
                                owl:allValuesFrom <http://www.w3.org/2006/time#TemporalEntity>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :oadrMinPeriod ;
                                owl:allValuesFrom <http://www.w3.org/2006/time#TemporalEntity>
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty :isOnChange ;
                                owl:someValuesFrom owl:rational
                              ] ;
              rdfs:label "Sampling rate" .


###  https://w3id.org/def/openadr#Signal
:Signal rdf:type owl:Class ;
        rdfs:subClassOf [ rdf:type owl:Restriction ;
                          owl:onProperty :hasSignalInterval ;
                          owl:someValuesFrom :SignalInterval
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasTarget ;
                          owl:someValuesFrom :Target
                        ] ;
        rdfs:comment "The actionable information contained in an event such as electricity pricing or specific levels of load shed requested that typically trigger some preprogrammed load shed behavior by the recipient of the event." ;
        rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
        rdfs:label "Signal" .


###  https://w3id.org/def/openadr#SignalInterval
:SignalInterval rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Restriction ;
                                  owl:onProperty :SignalIntervalDuration ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :SignalInterval
                                ] ,
                                [ rdf:type owl:Restriction ;
                                  owl:onProperty :hasTypicalValue ;
                                  owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                  owl:onClass :Value
                                ] ;
                rdfs:label "Interval" .


###  https://w3id.org/def/openadr#SimpleSignal
:SimpleSignal rdf:type owl:Class ;
              rdfs:subClassOf :Signal ;
              rdfs:label "Simple signal" .


###  https://w3id.org/def/openadr#Target
:Target rdf:type owl:Class ;
        rdfs:isDefinedBy "Target of a signal or event" ;
        rdfs:label "Target" .


###  https://w3id.org/def/openadr#TelemetryReport
:TelemetryReport rdf:type owl:Class ;
                 rdfs:subClassOf :DataReport ;
                 rdfs:label "Telemetry report" .


###  https://w3id.org/def/openadr#TelemetryStatusReport
:TelemetryStatusReport rdf:type owl:Class ;
                       rdfs:subClassOf :TelemetryReport ;
                       rdfs:label "Telemetry status report" .


###  https://w3id.org/def/openadr#TelemetryUsageReport
:TelemetryUsageReport rdf:type owl:Class ;
                      rdfs:subClassOf :TelemetryReport ;
                      rdfs:label "Telemetry usage report" .


###  https://w3id.org/def/openadr#VEN
:VEN rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :manages ;
                       owl:someValuesFrom :Resource
                     ] ;
     rdfs:comment "This is the OpenADR Virtual End Node that is used to interact with the VTN" ;
     rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
     rdfs:label "Virtual End Node (VEN)" .


###  https://w3id.org/def/openadr#VTN
:VTN rdf:type owl:Class ;
     rdfs:subClassOf [ rdf:type owl:Restriction ;
                       owl:onProperty :sendEvent ;
                       owl:someValuesFrom :Event
                     ] ;
     rdfs:comment "This is the OpenADR Virtual Top Node that is used to interact with the Resources enrolled in the DR Programs." ;
     rdfs:isDefinedBy "OpenADR 2.0 Demand Response Program Implementation Guide" ;
     rdfs:label "Virtual Top Node (VTN)" .


###  https://w3id.org/def/openadr#Value
:Value rdf:type owl:Class ;
       rdfs:subClassOf [ rdf:type owl:Restriction ;
                         owl:onProperty :isMeasuredIn ;
                         owl:someValuesFrom <http://www.wurvoc.org/vocabularies/om-1.8/Unit_of_measure>
                       ] ;
       rdfs:comment "Value of a signal in a particular interval" ;
       rdfs:label "Value" .


#################################################################
#    Individuals
#################################################################

###  https://w3id.org/def/openadr#Active
:Active rdf:type owl:NamedIndividual ,
                 :EventStatus .


###  https://w3id.org/def/openadr#Canceled
:Canceled rdf:type owl:NamedIndividual ,
                   :EventStatus .


###  https://w3id.org/def/openadr#Current
:Current rdf:type owl:NamedIndividual ,
                  :ItemBase .


###  https://w3id.org/def/openadr#EnergyAppartent
:EnergyAppartent rdf:type owl:NamedIndividual ,
